#!/bin/bash

if [ "$#" = "0" ]; then
	echo "Usage:"
	echo
	echo "./report-toolkit COMMAND FILENAME > OUTPUT_FILE"
	echo
	echo "Available commands are: keyword_cloud_from_export, emoji_cloud_from_export, get_text_from_export"
	echo
	echo "eg."
	echo "./report-toolkit emoji_cloud_from_export \$PWD/sample.csv > sample_emoji_cloud.txt"
	exit -1
fi

"$@"
